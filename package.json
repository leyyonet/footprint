{
  "name": "@leyyo/footprint",
  "version": "1.0.0",
  "description": "Object footprint",
  "keywords": [
    "foot print",
    "class parent",
    "class inheritance",
    "function types",
    "function parameter names",
    "object prototype"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/leyyonet/footprint.git"
  },
  "bugs": {
    "email": "mustafayelmer@gmail.com",
    "url": "https://github.com/leyyonet/footprint/issues"
  },
  "homepage": "https://github.com/leyyonet/footprint#readme",
  "author": {
    "name": "Mustafa Yelmer",
    "email": "mustafayelmer@gmail.com"
  },
  "main": "dist/index.js",
  "scripts": {
    "clear": "rimraf dist",
    "lint": "eslint src/**/*.ts",
    "build": "npm run clear && tsc && cp -rv src/assets dist/assets/",
    "test": "jest --config=jest.json --experimental-vm-modules",
    "test:watch": "jest --watch --config=jest.json",
    "test:coverage": "jest --config=jest.json --coverage --coverageDirectory=coverage",
    "start": "npm run lint && npm run build && npm run run:js",
    "run:js": "node dist/server.js",
    "run:ts": "node -r ts-node/register src/server.ts",
    "~publish": "npm run build && npm publish -access=public"
  },
  "files": [
    "dist/*"
  ],
  "license": "ISC",
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.5",
    "@types/node": "^22.13.4",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-node": "^11.1.0",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.3.1",
    "prettier": "^3.5.1",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.8.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@leyyo/common": "^1.0.4"
  }
}
